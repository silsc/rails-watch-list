<h1><%= @list.name %></h1>
<%= cl_image_tag(@list.photo.key,
      width: 400, height: 300, crop: :fill) if @list.photo.attached?  %>
<% @list.bookmarks.each do |bookmark| %>
  <%= bookmark.movie.title %>
  <%= bookmark.movie.overview %>
  <%= bookmark.movie.rating %>
  <%= link_to "Delete", bookmark_path(bookmark), method: :delete %>
<% end %>
<%= link_to "Back to lists", root_path %>
<%= link_to "Add a movie", new_list_bookmark_path(@list) %>
